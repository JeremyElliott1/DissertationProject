<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= pageTitle %></title>
  <script src="https://d3js.org/d3.v5.min.js"></script>

</head>

<body>

  <h1>Hello World</h1>
  <div id="data" data-xaxis="<%= xAxis %>">
  </div>

  <div id="chart">
    <p>test</p>
  </div>

</body>
<script>
  const dataDiv = document.querySelector('#data');
  let uninames = dataDiv.dataset.xaxis;
  uninames = JSON.parse(uninames); // NEED TO Unconvert from JSON obj to String for to manipulate the array.
  console.log(uninames);

  d3.select('#chart')
    .selectAll('p')
    .data(uninames)
    .enter()
    .append('p')
    .text(data => data);
</script>

</html>